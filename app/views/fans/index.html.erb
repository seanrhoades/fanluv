<p id="notice"><%= notice %></p>

<h1>Listing Fans</h1>

<% if current_fan %>
<h1> Welcome to FanLuv! </h1>
<% end %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fans.each do |fan| %>
      <tr><td><%=fan.name%></td></tr>
      <tr>
        <td><%= link_to 'Show', fan %></td>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fan', new_fan_path %>
