<p id="notice"><%= notice %></p>

<h1>Listing Luvs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @luvs.each do |luv| %>
      <tr>
        <td><%= link_to 'Show', luv %></td>
        <td><%= link_to 'Edit', edit_luv_path(luv) %></td>
        <td><%= link_to 'Destroy', luv, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Luv', new_luv_path %>
