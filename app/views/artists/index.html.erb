<p id="notice"><%= notice %></p>

<h1>Listing Artists</h1>

<% if current_artist %>
<h1> Welcome to FanLuv! </h1>
<% end %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr><td><%= artist.name %></td></tr>
      <tr>
        <td><%= link_to 'Show', artist %></td>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist', new_artist_path %>
