<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12 col-md-12">

      <h1>Log in</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag 'Log in'  %>
  </p>

</div>
</div>
</div>

<h3>New to FanLuv?  <%= link_to 'Click Here!', new_fan_path %></h3>
<% end %>
